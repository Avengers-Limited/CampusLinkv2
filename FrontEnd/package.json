{
  "name": "campuslink",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "start:tunnel": "expo start --tunnel",
    "start:clear": "expo start --clear",
    "start:tunnel:clear": "expo start --tunnel --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "cross-env EAS_NO_VCS=1 eas build --platform android --profile preview",
    "build:android:git": "eas build --platform android --profile preview",
    "build:ios": "cross-env EAS_NO_VCS=1 eas build --platform ios --profile preview",
    "build:ios:git": "eas build --platform ios --profile preview",
    "build:all": "cross-env EAS_NO_VCS=1 eas build --platform all --profile preview"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.8",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/native-stack": "^7.3.27",
    "@supabase/supabase-js": "^2.75.0",
    "expo": "~54.0.13",
    "expo-build-properties": "^1.0.9",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.21.0"
  },
  "private": true,
  "devDependencies": {
    "cross-env": "^10.1.0",
    "sharp": "^0.34.4"
  }
}
